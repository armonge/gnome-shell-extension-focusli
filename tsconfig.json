{
    "compilerOptions": {
        "outDir": "dist",
        "lib": [
            "ESNext"
        ],
        "types": [],
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "resolveJsonModule": true,
        "paths": {
            "resource:///org/gnome/shell/ui/slider.js": [
                "./node_modules/@girs/gnome-shell/src/ui/slider.d.ts"
            ],
            "resource:///org/gnome/shell/extensions/extension.js": [
                "./node_modules/@girs/gnome-shell/src/extensions/extension.d.ts"
            ],
            "resource:///org/gnome/shell/ui/main.js": [
                "./node_modules/@girs/gnome-shell/src/ui/main.d.ts"
            ],
            "resource:///org/gnome/shell/ui/panelMenu.js": [
                "./node_modules/@girs/gnome-shell/src/ui/panelMenu.d.ts"
            ],
            "resource:///org/gnome/shell/ui/popupMenu.js": [
                "./node_modules/@girs/gnome-shell/src/ui/popupMenu.d.ts"
            ],
            "gi://Gst": [
                "./node_modules/@girs/gst-1.0/gst-1.0.d.ts"
            ],
            "gi://GstAudio": [
                "./node_modules/@girs/gstaudio-1.0/gstaudio-1.0.d.ts"
            ],
            "gi://St": [
                "./@types/st-13.d.ts"
            ],
            "gi://Gio": [
                "./@types/gio-2.0.d.ts"
            ],
            "gi://GObject": [
                "./@types/gobject-2.0.d.ts"
            ],
            "gi://GLib": [
                "./@types/glib-2.0.d.ts"
            ]
        }
    },
    "include": [
        "./index.d.ts",
        "./node_modules/@girs/gjs/gjs.d.ts",
        "./node_modules/@girs/gjs/dom.d.ts"
    ],
    "files": [
        "src/extension.ts"
    ]
}
